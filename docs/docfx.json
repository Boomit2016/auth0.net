{
  "metadata": [
    {
      "src": [
        {
          "files": [ "Auth0.Core/Auth0.Core.csproj", "Auth0.AuthenticationApi/Auth0.AuthenticationApi.csproj", "Auth0.ManagementApi/Auth0.ManagementApi.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "cwd": "../src"
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "force": false,
    "content": [
      {
        "files": [ "**/*.yml" ],
        "src": "obj/api",
        "dest": "api"
      },
      {
        "files": [ "articles/**/*.md", "*.md", "toc.yml" ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "case": false,
        "noNegate": false,
        "noExpand": false,
        "noEscape": false,
        "noGlobStar": false,
        "dot": false
      }
    ],
    "externalReference": [],
    "dest": "_site",
    "template": [
      "default"
    ],
    "serve": false
  }
}